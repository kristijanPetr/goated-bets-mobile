{
  "name": "goated-bets",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "postinstall": "patch-package",
    "update:dev": "npx eas update --profile development",
    "build:local": "npx eas build --platform ios --local --profile development",
    "build:local:android": "npx eas build --platform android --local --profile development",
    "build:prod:ios": "npx eas build -p ios --clear-cache --profile production",
    "submit:prod": "npx eas submit --profile production --platform ios --latest"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@shopify/restyle": "^2.4.2",
    "date-fns": "^4.1.0",
    "expo": "^51.0.32",
    "expo-build-properties": "~0.12.5",
    "expo-dev-client": "~4.0.27",
    "expo-linear-gradient": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.25",
    "jsen-cls-sdk-prj-packagejs-mod-toolkit-pkg-interface-for-sdk-ecmascript": "./libraries/jsen-cls-sdk-prj-packagejs-mod-toolkit-pkg-interface-for-sdk-ecmascript",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.2.0",
    "react-content-loader": "^7.0.2",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "babel-plugin-inline-import": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "react-native-svg-transformer": "^1.5.0",
    "typescript": "~5.3.3"
  },
  "resolutions": {
    "react-native@0.74.5": "patch:react-native@npm:0.74.1#.yarn/patches/react-native-npm-0.74.1-754c02ae9e.patch"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "main": "node_modules/expo/AppEntry.js",
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "private": true
}
